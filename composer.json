{
    "name": "designbycode/active-links",
    "description": "Simple laravel helper utilities",
    "keywords": ["active-links", "menu active", "active class", "body class", "set classes"],
    "license": "MIT",
    "authors": [
        {
            "name": "Claude Myburgh",
            "email": "claude@deisgnbycode.co.za"
        }
    ],
    "require": {
        "php": "^7.2.5|^7.3|^7.4",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "DesignByCode\\ActiveLinks\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DesignByCode\\ActiveLinks\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {

        },
        "laravel": {
            "providers": [
                "DesignByCode\\ActiveLinks\\Providers\\ActiveLinksServiceProvider"
            ],
            "aliases": {
                "Set": "DesignByCode\\ActiveLinks\\Facades\\Active"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
